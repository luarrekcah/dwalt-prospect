{
  "name": "Prospect",
  "productName": "Prospect",
  "version": "1.2.0",
  "description": "Um programa windows para prospecção de clientes.",
  "main": "src/index.js",
  "build": {
    "appId": "com.dlwalt.prospect",
    "icon": "src/assets/icon.png",
    "productName": "Prospect",
    "files": [
      "dist/**/*",
      "src/assets/icon.*"
    ],
    "win": {
      "target": "nsis",
      "icon": "./src/assets/icon.co",
      "publisherName": "D | Walt Engenharia"
    },
    "compression": "maximum",
    "publish": [
      {
        "provider": "github",
        "owner": "luarrekcah",
        "repo": "dwalt-prospect"
      }
    ]
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-builder -p always",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "build": "rmdir /s /q Prospect-installers && rmdir /s /q Prospect-win32-x64 && electron-packager . --icon=src/assets/icon.ico && node build",
    "builder": "electron-builder build"
  },
  "keywords": [],
  "author": {
    "name": "luarrekcah",
    "email": "raulrodriguesdemoraes@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "electron-winstaller": "^5.1.0",
    "google-search-results-nodejs": "^2.1.0",
    "jquery": "^3.6.3",
    "puppeteer-core": "^19.7.1",
    "puppeteer-in-electron": "^3.0.5",
    "qr-image": "^3.2.0",
    "serpapi": "^1.1.1",
    "whatsapp-web.js": "^1.19.4",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "electron": "23.1.0",
    "electron-builder": "^23.6.0",
    "eslint": "^8.34.0",
    "eslint-config-google": "^0.14.0"
  }
}
